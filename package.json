{
  "name": "test-timeout",
  "version": "0.0.0",
  "description": "test-timeout",
  "main": "dist/frontend/bundle.js",
  "scripts": {
    "watch:frontend": "NODE_ENV=development webpack --config ./webpack.config.js --watch",
    "launch:dev": "NODE_ENV=development nodemon ./src/backend/main/index.js",
    "launch:prod": "NODE_ENV=production node ./src/backend/main/index.js",
    "compile:frontend": "NODE_ENV=production webpack --config ./webpack.config.js",
    "cleanup": "rm -rf ./dist/frontend",
    "start": "npm run launch:prod",
    "preversion": "npm test",
    "version": "npm run compile:frontend",
    "postversion": "git push && git push --tags"
  },
  "author": "Oleksii Honchar",
  "license": "MIT",
  "dependencies": {
    "axios": "0.16.2",
    "dot-prop-immutable": "1.3.1",
    "lodash": "4.17.4",
    "react": "15.5.4",
    "redux-actions": "2.2.1",
    "react-bootstrap": "0.31.3",
    "react-dom": "15.5.4",
    "react-fontawesome": "1.6.1",
    "react-redux": "5.0.5",
    "redux": "3.6.0",
    "redux-logger": "3.0.6",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "revalidator": "0.3.1",
    "styled-components": "2.1.1",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-relative-import": "1.0.3",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.24.1",
    "compression-webpack-plugin": "0.3.2",
    "copy-webpack-plugin": "4.0.1",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.10.1",
    "raw-loader": "0.5.1",
    "shelljs": "0.7.6",
    "snazzy": "7.0.0",
    "source-map-loader": "0.2.1",
    "standard": "10.0.2",
    "url-loader": "0.5.8",
    "webpack": "3.0.0",
    "webpack-bundle-analyzer": "2.8.2",
    "webpack-dev-server": "2.3.0",
    "webpack-merge": "4.1.0"
  },
  "browserslist": [
    "last 3 versions"
  ],
  "engines": {
    "node": "6.11.0"
  },
  "babel": {
    "presets": [
      "react",
      "stage-0",
      "env"
    ],
    "plugins": [
      [
        "babel-plugin-relative-import",
        {
          "rootPathSuffix": "src"
        }
      ],
      "transform-decorators-legacy",
      "transform-decorators",
      "transform-class-properties"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "browser": true,
      "mocha": true,
      "es6": true
    },
    "ignore": [
      "webpack*",
      "dist",
      "node_modules"
    ]
  }
}
